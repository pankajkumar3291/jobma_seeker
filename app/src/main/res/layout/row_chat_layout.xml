<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ECE8E5">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="8dp"
        android:background="#ECE8E5"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView2101"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="25/05/2019"
            android:textSize="@dimen/_9sdp"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout431"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout431" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout431"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10sdp"
            android:background="@drawable/chat_lyout_backgound_white"
            android:padding="@dimen/_10sdp"
            app:layout_constraintStart_toEndOf="@+id/circleImageView71"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView2091"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:maxWidth="@dimen/_160sdp"
                android:text="sgfgfgfgfgfgfg"
                android:textColor="@color/colorBlack"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView791"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:rotation="180"
            android:tint="@color/colorWhite"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout431"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout431"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout431"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout431"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_chat_blue_arrow" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:visibility="gone"
            android:id="@+id/circleImageView71"
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_marginStart="8dp"
            android:src="@drawable/no_image_available"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout431"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout431"
            app:layout_constraintVertical_bias="0.13" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/sendlayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#ECE8E5"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout44">

        <ProgressBar
            android:id="@+id/progressBar5"
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_marginEnd="8dp"
            android:visibility="gone"
            android:indeterminateDrawable="@drawable/progress_drawable"
            app:layout_constraintBottom_toTopOf="@+id/textView210"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout43"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView210"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="25/05/2019"
            android:textSize="@dimen/_9sdp"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout43"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout43" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout43"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10sdp"
            android:background="@drawable/chat_lyout_backgound_blue"
            android:padding="@dimen/_10sdp"
            app:layout_constraintEnd_toStartOf="@+id/circleImageView7"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView209"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:maxWidth="@dimen/_160sdp"
                android:text="fdfdfdfdfdfdfdf"
                android:textColor="@color/colorWhite"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView79"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="8dp"
            android:tint="#35DDFF"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout43"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout43"
            app:layout_constraintStart_toEndOf="@+id/constraintLayout43"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout43"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_chat_blue_arrow" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/circleImageView7"
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/no_image_available"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout43"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout43"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imageView80"
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:layout_marginEnd="8dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/constraintLayout43"
            app:layout_constraintEnd_toStartOf="@+id/progressBar5"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/delete_profile" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>